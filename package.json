    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.19.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mysql": "^2.18.1",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.7",
    "react-spring": "^8.0.27",
    "use-query-params": "^1.1.9"
  },
  "proxy": "http://localhost:5000"
